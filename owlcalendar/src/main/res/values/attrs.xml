<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OwlCalendar">
        <attr name="selected_date_background_color" format="color" />
        <attr name="today_text_color" format="color" />
        <attr name="day_text_color" format="color" />
        <attr name="day_name_text_color" format="color" />
        <attr name="date_text_color" format="color" />
        <attr name="line_background_color" format="color" />
        <attr name="picker_button_text_color" format="color" />
        <attr name="picker_button_text" format="string" />
        <attr name="mode" format="enum">
            <enum name="normal" value="0" />
            <enum name="single_selectable" value="1" />
            <enum name="range_selectable" value="2" />
        </attr>
    </declare-styleable>
</resources>